{
	"name": "Smart Fill Form",
	"version": "3.0",
	"manifest_version": 3,
	"description": "Auto fill form base on input type, name,...",
	"icons": { 
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png" 
	},
	"action": {
		"default_icon": "icon.png"
	},
	"background": { "service_worker": "background.js" },
	"permissions": [
	 	"clipboardWrite",
		"activeTab",
		"notifications",
		"contextMenus",
		"storage",
		"scripting"
	]
}
