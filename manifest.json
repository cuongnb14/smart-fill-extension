{
	"name": "Auto Fill",
	"version": "1.4",
	"manifest_version": 2,
	"description": "This chrome extension fills out forms automatically. It aims to make life for developers easier...",
	"icons": { 
		"16": "pen.png",
		"48": "pen.png",
		"128": "pen.png" 
	},
	"browser_action": {
		"default_icon": "pen.png"
	},
	"content_scripts": [
		{
			"matches" : [
				"http://*/*",
				"https://*/*"
			],
			"js": ["jquery-3.1.1.min.js", "faker.js"]
		}
	],
	"background": { "page": "background.html", "persistent": true },
	"permissions": [
	 	"clipboardWrite",
		"activeTab",
		"notifications",
		"contextMenus",
		"storage"
	]
}
